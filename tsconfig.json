  "compilerOptions": {
    "incremental": true,                              
    "target": "es5",                                     
    "module": "commonjs",                                
    "declaration": true,                              
    "outDir": "./lib",                                   
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                 
  }
  ,"include": ["src"]
}
